{
  "name": "neda-pay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "chmod +x ./vercel-install.sh && ./vercel-install.sh && NEXT_SKIP_TYPECHECKING=true NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "vercel-build-simple": "chmod +x ./vercel-build-simple.sh && ./vercel-build-simple.sh",
    "vercel-build-no-ts": "cp next.config.vercel.js next.config.js && NEXT_SKIP_TYPE_CHECK=true NEXT_SKIP_ESLINT=true NODE_OPTIONS=\"--max-old-space-size=8192\" next build --no-lint",
    "netlify-build": "chmod +x ./netlify-build.sh && ./netlify-build.sh"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.38.5",
    "@coinbase/wallet-sdk": "^4.3.2",
    "@tanstack/react-query": "^5.72.0",
    "@wagmi/core": "^2.16.7",
    "next": "14.2.15",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "viem": "^2.26.0",
    "wagmi": "^2.14.16"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
